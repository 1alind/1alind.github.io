{
  "number": 89,
  "name": "سُورَةُ الفَجۡرِ",
  "numberOfAyahs": 30,
  "verses": [
    { "numberInSurah": 1, "text": "وَٱلْفَجْرِ" },
    { "numberInSurah": 2, "text": "وَلَيَالٍ عَشْرٍ" },
    { "numberInSurah": 3, "text": "وَٱلشَّفْعِ وَٱلْوَتْرِ" },
    { "numberInSurah": 4, "text": "وَٱلَّيْلِ إِذَا يَسْرِ" },
    { "numberInSurah": 5, "text": "هَلْ فِى ذَٰلِكَ قَسَمٌ لِّذِى حِجْرٍ" },
    { "numberInSurah": 6, "text": "أَلَمْ تَرَ كَيْفَ فَعَلَ رَبُّكَ بِعَادٍ" },
    { "numberInSurah": 7, "text": "إِرَمَ ذَاتِ ٱلْعِمَادِ" },
    { "numberInSurah": 8, "text": "ٱلَّتِى لَمْ يُخْلَقْ مِثْلُهَا فِى ٱلْبِلَٰدِ" },
    { "numberInSurah": 9, "text": "وَثَمُودَ ٱلَّذِينَ جَابُوا۟ ٱلصَّخْرَ بِٱلْوَادِ" },
    { "numberInSurah": 10, "text": "وَفِرْعَوْنَ ذِى ٱلْأَوْتَادِ" },
    { "numberInSurah": 11, "text": "ٱلَّذِينَ طَغَوْا۟ فِى ٱلْبِلَٰدِ" },
    { "numberInSurah": 12, "text": "فَأَكْثَرُوا۟ فِيهَا ٱلْفَسَادَ" },
    { "numberInSurah": 13, "text": "فَصَبَّ عَلَيْهِمْ رَبُّكَ سَوْطَ عَذَابٍ" },
    { "numberInSurah": 14, "text": "إِنَّ رَبَّكَ لَبِٱلْمِرْصَادِ" },
    { "numberInSurah": 15, "text": "فَأَمَّا ٱلْإِنسَٰنُ إِذَا مَا ٱبْتَلَىٰهُ رَبُّهُۥ فَأَكْرَمَهُۥ وَنَعَّمَهُۥ فَيَقُولُ رَبِّىٓ أَكْرَمَنِ" },
    { "numberInSurah": 16, "text": "وَأَمَّآ إِذَا مَا ٱبْتَلَىٰهُ فَقَدَرَ عَلَيْهِ رِزْقَهُۥ فَيَقُولُ رَبِّىٓ أَهَٰنَنِ" },
    { "numberInSurah": 17, "text": "كَلَّا ۖ بَل لَّا تُكْرِمُونَ ٱلْيَتِيمَ" },
    { "numberInSurah": 18, "text": "وَلَا تَحَٰضُّونَ عَلَىٰ طَعَامِ ٱلْمِسْكِينِ" },
    { "numberInSurah": 19, "text": "وَتَأْكُلُونَ ٱلتُّرَاثَ أَكْلًا لَّمًّا" },
    { "numberInSurah": 20, "text": "وَتُحِبُّونَ ٱلْمَالَ حُبًّا جَمًّا" },
    { "numberInSurah": 21, "text": "كَلَّآ إِذَا دُكَّتِ ٱلْأَرْضُ دَكًّا دَكًّا" },
    { "numberInSurah": 22, "text": "وَجَآءَ رَبُّكَ وَٱلْمَلَكُ صَفًّا صَفًّا" },
    { "numberInSurah": 23, "text": "وَجِا۟ىٓءَ يَوْمَئِذٍۭ بِجَهَنَّمَ ۚ يَوْمَئِذٍ يَتَذَكَّرُ ٱلْإِنسَٰنُ وَأَنَّىٰ لَهُ ٱلذِّكْرَىٰ" },
    { "numberInSurah": 24, "text": "يَقُولُ يَٰلَيْتَنِى قَدَّمْتُ لِحَيَاتِى" },
    { "numberInSurah": 25, "text": "فَيَوْمَئِذٍ لَّا يُعَذِّبُ عَذَابَهُۥٓ أَحَدٌ" },
    { "numberInSurah": 26, "text": "وَلَا يُوثِقُ وَثَاقَهُۥٓ أَحَدٌ" },
    { "numberInSurah": 27, "text": "يَٰٓأَيَّتُهَا ٱلنَّفْسُ ٱلْمُطْمَئِنَّةُ" },
    { "numberInSurah": 28, "text": "ٱرْجِعِىٓ إِلَىٰ رَبِّكِ رَاضِيَةً مَّرْضِيَّةً" },
    { "numberInSurah": 29, "text": "فَٱدْخُلِى فِى عِبَٰدِى" },
    { "numberInSurah": 30, "text": "وَٱدْخُلِى جَنَّتِى" }
  ]
}