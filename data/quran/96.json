  "verses": [
    { "numberInSurah": 1, "text": "ٱقْرَأْ بِٱسْمِ رَبِّكَ ٱلَّذِى خَلَقَ" },
    { "numberInSurah": 2, "text": "خَلَقَ ٱلْإِنسَٰنَ مِنْ عَلَقٍ" },
    { "numberInSurah": 3, "text": "ٱقْرَأْ وَرَبُّكَ ٱلْأَكْرَمُ" },
    { "numberInSurah": 4, "text": "ٱلَّذِى عَلَّمَ بِٱلْقَلَمِ" },
    { "numberInSurah": 5, "text": "عَلَّمَ ٱلْإِنسَٰنَ مَا لَمْ يَعْلَمْ" },
    { "numberInSurah": 6, "text": "كَلَّآ إِنَّ ٱلْإِنسَٰنَ لَيَطْغَىٰٓ" },
    { "numberInSurah": 7, "text": "أَن رَّءَاهُ ٱسْتَغْنَىٰٓ" },
    { "numberInSurah": 8, "text": "إِنَّ إِلَىٰ رَبِّكَ ٱلرُّجْعَىٰٓ" },
    { "numberInSurah": 9, "text": "أَرَءَيْتَ ٱلَّذِى يَنْهَىٰ" },
    { "numberInSurah": 10, "text": "عَبْدًا إِذَا صَلَّىٰٓ" },
    { "numberInSurah": 11, "text": "أَرَءَيْتَ إِن كَانَ عَلَى ٱلْهُدَىٰٓ" },
    { "numberInSurah": 12, "text": "أَوْ أَمَرَ بِٱلتَّقْوَىٰٓ" },
    { "numberInSurah": 13, "text": "أَرَءَيْتَ إِن كَذَّبَ وَتَوَلَّىٰٓ" },
    { "numberInSurah": 14, "text": "أَلَمْ يَعْلَم بِأَنَّ ٱللَّهَ يَرَىٰ" },
    { "numberInSurah": 15, "text": "كَلَّا لَئِن لَّمْ يَنتَهِ لَنَسْفَعًۢا بِٱلنَّاصِيَةِ" },
    { "numberInSurah": 16, "text": "نَاصِيَةٍ كَٰذِبَةٍ خَاطِئَةٍ" },
    { "numberInSurah": 17, "text": "فَلْيَدْعُ نَادِيَهُۥ" },
    { "numberInSurah": 18, "text": "سَنَدْعُ ٱلزَّبَانِيَةَ" },
    { "numberInSurah": 19, "text": "كَلَّا لَا تُطِعْهُ وَٱسْجُدْ وَٱقْتَرِب" }
  ]
