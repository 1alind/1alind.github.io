{
  "number": 88,
  "name": "سُورَةُ الغَاشِيَةِ",
  "numberOfAyahs": 26,
  "verses": [
    { "numberInSurah": 1, "text": "هَلْ أَتَىٰكَ حَدِيثُ ٱلْغَشِيَةِ" },
    { "numberInSurah": 2, "text": "وُجُوهٌ يَوْمَئِذٍ خَٰشِعَةٌ" },
    { "numberInSurah": 3, "text": "عَامِلَةٌ نَّاصِبَةٌ" },
    { "numberInSurah": 4, "text": "تَصْلَىٰ نَارًا حَامِيَةً" },
    { "numberInSurah": 5, "text": "تُسْقَىٰ مِنْ عَيْنٍ ءَانِيَةٍ" },
    { "numberInSurah": 6, "text": "لَّيْسَ لَهُمْ طَعَامٌ إِلَّا مِن ضَرِيعٍ" },
    { "numberInSurah": 7, "text": "لَّا يُسْمِنُ وَلَا يُغْنِى مِن جُوعٍ" },
    { "numberInSurah": 8, "text": "وُجُوهٌ يَوْمَئِذٍ نَّاعِمَةٌ" },
    { "numberInSurah": 9, "text": "لِّسَعْيِهَا رَاضِيَةٌ" },
    { "numberInSurah": 10, "text": "فِى جَنَّةٍ عَالِيَةٍ" },
    { "numberInSurah": 11, "text": "لَّا تَسْمَعُ فِيهَا لَٰغِيَةً" },
    { "numberInSurah": 12, "text": "فِيهَا عَيْنٌ جَارِيَةٌ" },
    { "numberInSurah": 13, "text": "فِيهَا سُرُرٌ مَّرْفُوعَةٌ" },
    { "numberInSurah": 14, "text": "وَأَكْوَابٌ مَّوْضُوعَةٌ" },
    { "numberInSurah": 15, "text": "وَنَمَارِقُ مَصْفُوفَةٌ" },
    { "numberInSurah": 16, "text": "وَزَرَابِىُّ مَبْثُوثَةٌ" },
    { "numberInSurah": 17, "text": "أَفَلَا يَنظُرُونَ إِلَى ٱلْإِبِلِ كَيْفَ خُلِقَتْ" },
    { "numberInSurah": 18, "text": "وَإِلَى ٱلسَّمَآءِ كَيْفَ رُفِعَتْ" },
    { "numberInSurah": 19, "text": "وَإِلَى ٱلْجِبَالِ كَيْفَ نُصِبَتْ" },
    { "numberInSurah": 20, "text": "وَإِلَى ٱلْأَرْضِ كَيْفَ سُطِحَتْ" },
    { "numberInSurah": 21, "text": "فَذَكِّرْ إِنَّمَآ أَنتَ مُذَكِّرٌ" },
    { "numberInSurah": 22, "text": "لَّسْتَ عَلَيْهِم بِمُصَيْطِرٍ" },
    { "numberInSurah": 23, "text": "إِلَّا مَن تَوَلَّىٰ وَكَفَرَ" },
    { "numberInSurah": 24, "text": "فَيُعَذِّبُهُ ٱللَّهُ ٱلْعَذَابَ ٱلْأَكْبَرَ" },
    { "numberInSurah": 25, "text": "إِنَّ إِلَيْنَآ إِيَابَهُمْ" },
    { "numberInSurah": 26, "text": "ثُمَّ إِنَّ عَلَيْنَا حِسَابَهُم" }
  ]
}