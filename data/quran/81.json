{
  "number": 81,
  "name": "سُورَةُ التَّكۡوِيرِ",
  "numberOfAyahs": 29,
  "verses": [
    { "numberInSurah": 1, "text": "إِذَا ٱلشَّمْسُ كُوِّرَتْ" },
    { "numberInSurah": 2, "text": "وَإِذَا ٱلنُّجُومُ ٱنكَدَرَتْ" },
    { "numberInSurah": 3, "text": "وَإِذَا ٱلْجِبَالُ سُيِّرَتْ" },
    { "numberInSurah": 4, "text": "وَإِذَا ٱلْعِشَارُ عُطِّلَتْ" },
    { "numberInSurah": 5, "text": "وَإِذَا ٱلْوُحُوشُ حُشِرَتْ" },
    { "numberInSurah": 6, "text": "وَإِذَا ٱلْبِحَارُ سُجِّرَتْ" },
    { "numberInSurah": 7, "text": "وَإِذَا ٱلنُّفُوسُ زُوِّجَتْ" },
    { "numberInSurah": 8, "text": "وَإِذَا ٱلْمَوْءُۥدَةُ سُئِلَتْ" },
    { "numberInSurah": 9, "text": "بِأَىِّ ذَنۢبٍ قُتِلَتْ" },
    { "numberInSurah": 10, "text": "وَإِذَا ٱلصُّحُفُ نُشِرَتْ" },
    { "numberInSurah": 11, "text": "وَإِذَا ٱلسَّمَآءُ كُشِطَتْ" },
    { "numberInSurah": 12, "text": "وَإِذَا ٱلْجَحِيمُ سُعِّرَتْ" },
    { "numberInSurah": 13, "text": "وَإِذَا ٱلْجَنَّةُ أُزْلِفَتْ" },
    { "numberInSurah": 14, "text": "عَلِمَتْ نَفْسٌ مَّآ أَحْضَرَتْ" },
    { "numberInSurah": 15, "text": "فَلَآ أُقْسِمُ بِٱلْخُنَّسِ" },
    { "numberInSurah": 16, "text": "ٱلْجَوَارِ ٱلْكُنَّسِ" },
    { "numberInSurah": 17, "text": "وَٱلَّيْلِ إِذَا عَسْعَسَ" },
    { "numberInSurah": 18, "text": "وَٱلصُّبْحِ إِذَا تَنَفَّسَ" },
    { "numberInSurah": 19, "text": "إِنَّهُۥ لَقَوْلُ رَسُولٍ كَرِيمٍ" },
    { "numberInSurah": 20, "text": "ذِى قُوَّةٍ عِندَ ذِى ٱلْعَرْشِ مَكِينٍ" },
    { "numberInSurah": 21, "text": "مُّطَاعٍ ثَمَّ أَمِينٍ" },
    { "numberInSurah": 22, "text": "وَمَا صَاحِبُكُم بِمَجْنُونٍ" },
    { "numberInSurah": 23, "text": "وَلَقَدْ رَءَاهُ بِٱلْأُفُقِ ٱلْمُبِينِ" },
    { "numberInSurah": 24, "text": "وَمَا هُوَ عَلَى ٱلْغَيْبِ بِضَنِينٍ" },
    { "numberInSurah": 25, "text": "وَمَا هُوَ بِقَوْلِ شَيْطَٰنٍ رَّجِيمٍ" },
    { "numberInSurah": 26, "text": "فَأَيْنَ تَذْهَبُونَ" },
    { "numberInSurah": 27, "text": "إِنْ هُوَ إِلَّا ذِكْرٌ لِّلْعَٰلَمِينَ" },
    { "numberInSurah": 28, "text": "لِمَن شَآءَ مِنكُمْ أَن يَسْتَقِيمَ" },
    { "numberInSurah": 29, "text": "وَمَا تَشَآءُونَ إِلَّآ أَن يَشَآءَ ٱللَّهُ رَبُّ ٱلْعَٰلَمِينَ" }
  ]
}