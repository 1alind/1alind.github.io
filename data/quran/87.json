{
  "number": 87,
  "name": "سُورَةُ الأَعۡلَىٰ",
  "numberOfAyahs": 19,
  "verses": [
    { "numberInSurah": 1, "text": "سَبِّحِ ٱسْمَ رَبِّكَ ٱلْأَعْلَى" },
    { "numberInSurah": 2, "text": "ٱلَّذِى خَلَقَ فَسَوَّىٰ" },
    { "numberInSurah": 3, "text": "وَٱلَّذِى قَدَّرَ فَهَدَىٰ" },
    { "numberInSurah": 4, "text": "وَٱلَّذِىٓ أَخْرَجَ ٱلْمَرْعَىٰ" },
    { "numberInSurah": 5, "text": "فَجَعَلَهُۥ غُثَآءً أَحْوَىٰ" },
    { "numberInSurah": 6, "text": "سَنُقْرِئُكَ فَلَا تَنسَىٰٓ" },
    { "numberInSurah": 7, "text": "إِلَّا مَا شَآءَ ٱللَّهُ ۚ إِنَّهُۥ يَعْلَمُ ٱلْجَهْرَ وَمَا يَخْفَىٰ" },
    { "numberInSurah": 8, "text": "وَنُيَسِّرُكَ لِلْيُسْرَىٰ" },
    { "numberInSurah": 9, "text": "فَذَكِّرْ إِن نَّفَعَتِ ٱلذِّكْرَىٰ" },
    { "numberInSurah": 10, "text": "سَيَذَّكَّرُ مَن يَخْشَىٰ" },
    { "numberInSurah": 11, "text": "وَيَتَجَنَّبُهَا ٱلْأَشْقَى" },
    { "numberInSurah": 12, "text": "ٱلَّذِى يَصْلَى ٱلنَّارَ ٱلْكُبْرَىٰ" },
    { "numberInSurah": 13, "text": "ثُمَّ لَا يَمُوتُ فِيهَا وَلَا يَحْيَىٰ" },
    { "numberInSurah": 14, "text": "قَدْ أَفْلَحَ مَن تَزَكَّىٰ" },
    { "numberInSurah": 15, "text": "وَذَكَرَ ٱسْمَ رَبِّهِۦ فَصَلَّىٰ" },
    { "numberInSurah": 16, "text": "بَلْ تُؤْثِرُونَ ٱلْحَيَوٰةَ ٱلدُّنْيَا" },
    { "numberInSurah": 17, "text": "وَٱلْـَٔاخِرَةُ خَيْرٌ وَأَبْقَىٰٓ" },
    { "numberInSurah": 18, "text": "إِنَّ هَٰذَا لَفِى ٱلصُّحُفِ ٱلْأُولَىٰ" },
    { "numberInSurah": 19, "text": "صُحُفِ إِبْرَٰهِيمَ وَمُوسَىٰ" }
  ]
}